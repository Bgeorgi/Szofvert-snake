<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="900.0" spacing="20.0" style="-fx-background-image: url('/images/background.jpg');" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ScoreController">
    <children>
      <Pane prefHeight="700.0" prefWidth="900.0">
         <children>
              <Button fx:id="backButton" layoutX="34.0" layoutY="35.0" onMouseClicked="#switchToMenu" prefHeight="33.0" prefWidth="80.0" style="-fx-background-color: brown;" text="Back" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
              <Label layoutX="320.0" layoutY="32.0" style="-fx-background-color: white;" text="Score Board" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="32.0" />
               </font>
            </Label>
            <TableView fx:id="table" layoutX="9.0" layoutY="183.0" prefHeight="447.0" prefWidth="838.0">
              <columns>
                <TableColumn fx:id="tablePlayer" prefWidth="458.0" text="Player" />
                <TableColumn fx:id="tableScore" minWidth="0.0" prefWidth="379.0" text="Score" />
              </columns>
            </TableView>
         </children>
      </Pane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
