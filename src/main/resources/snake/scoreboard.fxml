<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="900.0" spacing="20.0" style="-fx-background-image: url('/images/background.jpg');" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="snake.ScoreController">
    <children>
      <Pane prefHeight="700.0" prefWidth="900.0">
         <children>
              <Button fx:id="backButton" layoutX="34.0" layoutY="35.0" onMouseClicked="#switchToMenu" prefHeight="33.0" prefWidth="80.0" style="-fx-background-color: brown;" text="Back" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
              <Label layoutX="320.0" layoutY="32.0" style="-fx-background-color: white;" text="Score Board" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="32.0" />
               </font>
            </Label>
            <TextArea fx:id="DataScore" layoutX="30.0" layoutY="222.0" prefHeight="400.0" prefWidth="800.0" />
         </children>
      </Pane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
